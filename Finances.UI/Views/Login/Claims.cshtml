@model List<Finances.UI.Models.ClaimViewModel>
@{
    ViewData["Title"] = "Claims";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title m-b-0">Hakkımızda</h5>
            </div>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Dağıtan</th>
                        <th scope="col">Type</th>
                        <th scope="col">Value</th>
                        <th scope="col">Sil</th>
                        <th scope="col">Güncelle</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <th scope="row">@item.Issuer</th>
                            <td>@item.Type</td>
                            <td>@item.Value</td>
                        </tr>
                    }


            </table>

        </div>
        <br />
    </div>
</div>

