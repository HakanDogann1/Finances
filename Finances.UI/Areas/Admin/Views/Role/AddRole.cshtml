@model Finances.UI.Models.AddRoleViewModel
@{
    ViewData["Title"] = "AddRole";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}
<div class="card">
    <div class="card-body wizard-content">
        <h4 class="card-title">Rol Ekleme Sayfası</h4>
        <h6 class="card-subtitle"></h6>
        <form method="post">
            <div>
                <section>
                    <label for="userName">Rol Adı </label>
                    <input type="text" class="required form-control" asp-for="RoleName">
                </section>
                <br />
                <button class="btn btn-outline-success">Ekle</button>
            </div>
        </form>
    </div>
</div>



