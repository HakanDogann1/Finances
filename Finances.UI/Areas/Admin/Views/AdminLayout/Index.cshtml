
@{
    Layout = null;
}

<!DOCTYPE html>
<html dir="ltr" lang="en">

@await Html.PartialAsync("~/Areas/Admin/Views/AdminLayout/HeadPartial.cshtml");
<body>
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
